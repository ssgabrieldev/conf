{
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<tab>"],
            "after": ["g", "t"]
        },
        {
            "before": ["<s-tab>"],
            "after": ["g", "T"]
        }
    ],
    "vim.handleKeys": {
        "<C-b>": false,
        "<C-j>": false
    },
    "explorer.confirmDelete": false,
    "editor.cursorStyle": "block",
    "extensions.ignoreRecommendations": true,
    "git.openRepositoryInParentFolders": "always"
}

